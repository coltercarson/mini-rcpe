[run]
source = .
omit = 
    tests/*
    test_*.py
    venv/*
    */site-packages/*
    */dist-packages/*
    __pycache__/*
    .venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
